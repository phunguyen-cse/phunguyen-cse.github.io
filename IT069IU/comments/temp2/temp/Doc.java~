import java.util.Scanner;

/**
 * class description
 * @version 1.0
 * @author Lin Chen
 */

public class Doc
{
	/**
	 * Doc name
	 */
	private String name;

	/**
	 * Constructor for Doc class
	 */
	public Doc()
	{
	}

	/** Get doc name
	 * @return doc name
	 */
	public String getName()
	{
		return name;
	}

	/** Set doc name
	 * @param doc name
	 */
	public void setName(String s)
	{
		this.name = s;
	}
}
